{
    "name": "mep_project/php_benchmark_runner",
    "description": "A PHP benchmark runner inspired by PHPBench",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MepProject\\PhpBenchmarkRunner\\": "src/",
            "MepProject\\PhpBenchmarkRunner\\Service\\": "src/Service/",
            "MepProject\\PhpBenchmarkRunner\\Exception\\": "src/Exception/",
            "MepProject\\PhpBenchmarkRunner\\DTO\\": "src/DTO/",
            "MepProject\\PhpBenchmarkRunner\\DTO\\Abstractions\\": "src/DTO/Abstractions"
        }
    },
    "require": {
        "php": "^7.4.0",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2"
    },
    "authors": [
        {
            "name": "Catalina Anghel",
            "role": "Developer"
        }
    ]
}
